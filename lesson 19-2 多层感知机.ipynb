    "import torch\n",
    "from torch.nn import functional as F\n",
    "x=torch.randn(1,10)\n",
    "w=torch.randn(2,10,requires_grad=True)#10个输入 两个输出的感知机\n",
    "o=torch.sigmoid(x@w.t())\n",
    "loss=F.mse_loss(torch.ones(1,2),o)\n",
    "loss"
   ]
  },
